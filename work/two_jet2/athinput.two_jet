<comment>
problem   = underdense Mach10 jet
reference = 
configure = --prob=two_jet

<job>
problem_id = two_jet      # problem ID: basename of output filenames


<output1>
file_type  = hst      # History data dump
dt         = 0.1      # time increment between outputs

<output2>
file_type  = hdf5     
variable   = prim     # variables to be output
dt         = 1.0      # time increment between outputs
ghost_zones = true

<time>
cfl_number = 0.4      # The Courant, Friedrichs, & Lewy (CFL) Number
nlim       = -1       # cycle limit
tlim       = 300.0    # time limit
integrator  = vl2       # time integration algorithm
xorder      = 2         # order of spatial reconstruction
ncycle_out  = 1         # interval for stdout summary info

<mesh>
nx1        = 100      # Number of zones in X1-direction
x1min      = 0.0      # minimum value of X1
x1max      = 60.0     # maximum value of X1
ix1_bc     = user     # inner-X1 boundary flag
ox1_bc     = user     # outer-X1 boundary flag

nx2        = 250      # Number of zones in X2-direction
x2min      = -25.0    # minimum value of X2
x2max      = 25.0     # maximum value of X2
ix2_bc     = outflow  # inner-X2 boundary flag
ox2_bc     = outflow  # outer-X2 boundary flag

nx3        = 1        # Number of zones in X3-direction
x3min      = -25.0    # minimum value of X3
x3max      = 25.0     # maximum value of X3
ix3_bc     = outflow  # inner-X3 boundary flag
ox3_bc     = outflow  # outer-X3 boundary flag

<hydro>
gamma   = 1.666666666667     # gamma = C_p/C_v

<problem>
d  = 1.0
p  = 6.0e-3
vx = 0.0
vy = 0.0
vz = 0.0
bx = 0.1
by = 0.0
bz = 0.0

djet_L = 0.1
pjet_L = 6.0e-3
vxjet_L = 1.0
vyjet_L = 0.0
vzjet_L = 0.0 
bxjet_L = 0.1
byjet_L = 0.0
bzjet_L = 0.0

rjet_L = 1.0

djet_R = 0.1
pjet_R = 6.0e-3
vxjet_R = -1.0
vyjet_R = 0.0
vzjet_R = 0.0 
bxjet_R = 0.1
byjet_R = 0.0
bzjet_R = 0.0

rjet_R = 1.0
